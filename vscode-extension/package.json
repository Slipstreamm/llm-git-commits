{
  "name": "llm-git-commits",
  "displayName": "LLM Git Commits",
  "description": "Generate commit messages with LLM and commit from VS Code",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:llmGitCommits.generateCommit"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "llmGitCommits.generateCommit",
        "title": "LLM Git: Generate Commit"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ."
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.101.0",
    "typescript": "^5.0.0"
  }
}
